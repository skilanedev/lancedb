namespace lancedb {

dictionary Field {
    string name;
    string data_type;
    boolean nullable;
}

dictionary Schema {
    sequence<Field> fields;
}

[Error]
error LanceError {
    string message;
}

[Async]
interface Database {
    constructor(string uri) raises (LanceError);
    [Async] void createTable(string name, Schema schema) raises (LanceError);
}

}